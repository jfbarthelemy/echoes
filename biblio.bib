@article{abou-chakraguery2009,
  title = {Application of a Micromechanical Model to Cavity Excavation Analysis in Argillite},
  author = {{Abou-Chakra Gu{\'e}ry}, A. and Cormery, F. and Shao, J. -F. and Kondo, D.},
  year = {2009},
