@article{lu2019,
  title = {Multiscale Study of Influence of Interfacial Decohesion on Piezoresistivity of Graphene/Polymer Nanocomposites},
  author = {Lu, Xiaoxin and Detrez, Fabrice and Yvonnet, Julien and Bai, Jinbo},
  year = {2019},
  journal = {Modelling and Simulation in Materials Science and Engineering},
