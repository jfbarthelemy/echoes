@article{moakher2006,
  title = {The {{Closest Elastic Tensor}} of {{Arbitrary Symmetry}} to an {{Elasticity Tensor}} of {{Lower Symmetry}}},
  author = {Moakher, Maher and Norris, Andrew N.},
  year = {2006},
