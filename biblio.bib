  journal = {International Journal of Engineering Science},
  volume = {147},
  pages = {103196},
  issn = {00207225},
  doi = {10.1016/j.ijengsci.2019.103196},
  abstract = {State-of-the-art micromechanical models for cement and concrete typically introduce two types of hydrate gel built up by single nanometer-sized solid calcium-silicate-hydrate (C-S-H) blocks and a constant gel porosity in between, in order to upscale nanoindentation-derived elastic properties of ''low density C-S-H gel'' and ''high-density C-S-H gel'' up the level of cement paste and concrete. Recently, the vision of only two types of hydrates with two different, but constant mass densities has been severely challenged by Nuclear Magnetic Resonance relaxometry experiments, suggesting hydrates which continuously densify during the hydration process. We here provide a comprehensive hierarchical continuum micromechanics framework for a sequence of material volumes with infinitely many, eigenstressed phases oriented in all space directions. This novel scheme, resting largely on Dvorak's transformation field analysis coupled with eigenstressed matrix-inhomogeneity problems of the Eshelby-Laws type, is sophisticated enough to translate, in a very precise fashion, both the molecular mechanics-proven elasticity of solid C-S-H and the precipitation space-driven evolution of gel porosity within shape-changing hydrate gel phases, into the elastic and poro-elastic properties of cement paste.},
  keywords = {Calcium-silicate-hydrate (C-S-H),Cement,Densification,Matrix-inclusion problem,Modeling,Poro-micromechanics,Principle of virtual power,Random homogenization theory,Transformation field analysis},
  file = {..\\..\\..\\Documents\\CTOA\\Biblio\\Beton hydratation\\Koenigsberge_IJES2020.pdf}
}

@article{konigsberger2021,
