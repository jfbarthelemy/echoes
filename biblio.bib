@article{scheel2021,
  title = {A {{Critical Review}} on the {{Complex Potentials}} in {{Linear Elastic Fracture Mechanics}}},
  author = {Scheel, Johannes and Wallenta, Daniel and Ricoeur, Andreas},
  year = {2021},
  month = dec,
  journal = {Journal of Elasticity},
  volume = {147},
  number = {1-2},
  pages = {291--308},
  publisher = {{The Author(s)}},
  issn = {0374-3535},
  doi = {10.1007/s10659-021-09873-1},
  abstract = {Introducing a crack in an elastic plate is challenging from the mathematical point of view and relevant within an engineering context of evaluating strength and reliability of structures. Accordingly, a multitude of associated works is available to date, emanating from both applied mathematics and mechanics communities. Although considering the same problem, the given complex potentials prove to be different, revealing various inconsistencies in terms of resulting stresses and displacements. Essential information on crack near-tip fields and crack opening displacements is nonetheless available, while intuitive adaption is required to obtain the full-field solutions. Investigating the cause of prevailing deficiencies inevitably leads to a critical review of classical works by Muskhelishvili or Westergaard. Complex potentials of the mixed-mode loaded Griffith crack, sparing restrictive assumptions or limitations of validity, are finally provided, allowing for rigorous mathematical treatment. The entity of stresses and displacements in the whole plate is finally illustrated and the distributions in the crack plane are given explicitly.},
  keywords = {2010,30a99,74b05,74r10,complex potentials,Complex potentials,crack fields,Crack fields,griffith crack,Griffith crack,holomorphic functions,Holomorphic functions,kolosov,Kolosov's equations,mathematics subject classification,s equations,westergaard stress function,Westergaard stress function},
  file = {..\\..\\..\\Documents\\CTOA\\Biblio\\Homogeneisation\\Scheel2021_Article_ACriticalReviewOnTheComplexPot.pdf}
}

@article{schneider2019,
  title = {On the Mathematical Foundations of the Self-Consistent Clustering Analysis for Non-Linear Materials at Small Strains},
  author = {Schneider, Matti},
  year = {2019},
  month = sep,
  journal = {Computer Methods in Applied Mechanics and Engineering},
  volume = {354},
  pages = {783--801},
  publisher = {{Elsevier B.V.}},
  issn = {00457825},
  doi = {10.1016/j.cma.2019.06.003},
  abstract = {We investigate, both mathematically and numerically, the self-consistent clustering analysis recently introduced by Liu\textendash Bessa\textendash Liu and, independently, by Wulfinghoff\textendash Cavaliere\textendash Reese. We establish, in the small strain setting and non-softening material behavior, existence and uniqueness of the solution to the discretized equations for fixed (possibly anisotropic) reference material, by a constructive method. Furthermore, we establish convergence of the solution to the discretized equation to the continuous solution upon refinement of the clusters. Thus we generalize the work of Tang\textendash Zhang\textendash Liu to spatial dimensions larger than 1. We explore the specific structure of the Lippmann\textendash Schwinger equation governing clustering analysis, proving strict equivalence to a problem of Eyre\textendash Milton type. For the latter formulation, existence and uniqueness are easily established based on recent progress in the understanding of polarization schemes in FFT-based computational homogenization methods. Last but not least, for elasto-viscoplastic constituents we clarify the relationship of the self-consistent clustering analysis to the transformation field analysis of Dvorak\textendash Benveniste. Our theoretical considerations are confirmed by pertinent numerical experiments.},
  keywords = {Composites,Computational micromechanics,Hashin–Shtrikman variational principle,Lippmann–Schwinger equation,Self-consistent clustering analysis,Transformation field analysis},
  file = {..\\..\\..\\Documents\\CTOA\\Biblio\\Homogeneisation\\schn2019b.pdf}
}

