@article{francois1998,
  title = {Determination of the Symmetries of an Experimentally Determined Stiffness Tensor: {{Application}} to Acoustic Measurements},
  author = {Fran{\c c}ois, M. and Geymonat, G. and Berthaud, Y.},
  year = {1998},
  month = nov,
  journal = {International Journal of Solids and Structures},
  volume = {35},
  number = {31-32},
  pages = {4091--4106},
  issn = {00207683},
  doi = {10.1016/S0020-7683(97)00303-X},
  abstract = {For most materials, the symmetry group is known a priori and deduced from the realization process. This allows many simplifications for the measurements of the stiffness tensor. We deal here with the case where the symmetry is a priori unknown, as for biological or geological materials, or when the process makes the material symmetry axis uncertain (some composites, monocrystals). The measurements are then more complicated and the raw stiffness tensor obtained does not exhibit any symmetry in the Voigt's matrical form, as it is expressed in the arbitrarily chosen specimen's base. A complete ultrasonic measurement of this stiffness tensor from redundant measurements is proposed. In a second time, we show how to make a plane symmetry pole figure able to give visual information about the quasi-symmetries of a raw stiffness tensor determined by any measurement method. Finally we introduce the concept of distance from a raw stiffness tensor to one of the eight symmetry classes available for a stiffness tensor. The method provides the nearest tensor (to the raw stiffness tensor) possessing a chosen symmetry class, with its associated natural symmetry base. \textcopyright{} 1998 Elsevier Science Ltd. All rights reserved.},
  file = {..\\..\\..\\Documents\\CTOA\\Biblio\\Symmetries\\Francois_IJSS1998.pdf}
}

@article{francois2018,
