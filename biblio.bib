@article{fricke1924,
  title = {A {{Mathematical Treatment}} of the {{Electric Conductivity}} and {{Capacity}} of {{Disperse Systems I}}. {{The Electric Conductivity}} of a {{Suspension}} of {{Homogeneous Spheroids}}},
  author = {Fricke, Hugo},
