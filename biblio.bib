@article{samantray2021,
  title = {Micro-Mechanical Modeling of Irreversible Hygroscopic Strain in Paper Sheets Exposed to Moisture Cycles},
  author = {Samantray, P. and Peerlings, R.H.J. and Massart, T.J. and Geers, M.G.D.},
  year = {2021},
