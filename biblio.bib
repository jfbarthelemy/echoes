@article{konigsberger2020,
  title = {Multiscale Poro-Elasticity of Densifying Calcium-Silicate Hydrates in Cement Paste: {{An}} Experimentally Validated Continuum Micromechanics Approach},
  author = {K{\"o}nigsberger, Markus and Pichler, Bernhard and Hellmich, Christian},
