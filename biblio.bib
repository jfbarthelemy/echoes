@article{fares2018,
  title = {Determining Chloride Content Profiles in Concrete Using an Electrical Resistivity Tomography Device},
  author = {Fares, Milia and Villain, G{\'e}raldine and Bonnet, St{\'e}phanie and Palma Lopes, S{\'e}rgio and Thauvin, Benoit and Thiery, Micka{\"e}l},
